<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="superSocket"
         type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine" />
  </configSections>
  <appSettings>
    <add key="ServiceName" value="SuperSocket"/>
  </appSettings>
  <superSocket>
    <servers>
      <server name="QCP.Center"
          serverTypeName="QCP.Center"
          ip="Any" port="2020">
      </server>
    </servers>
    <serverTypes>
      <add name="QCP.Center" type="Cube.Net.Server.CubeServer, Cube.Net.Server"/>
    </serverTypes>
  </superSocket>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>